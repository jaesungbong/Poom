<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout1"
    android:layout_width="288dp"
    android:layout_height="294dp"
    android:background="@drawable/donate_pop_bg"
    android:minHeight="294dp"
    android:minWidth="288dp" >

    <Button
        android:id="@+id/recharge"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_gravity="top|right"
        android:layout_marginRight="25dp"
        android:layout_marginTop="71dp"
        android:background="@drawable/donate_pop_btn_plus"
        android:text="" />

    <EditText
        android:id="@+id/energy_share_dialog_text"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="151dp"
        android:background="@drawable/donate_pop_input_off"
        android:ems="10"
        android:inputType="number"
        android:gravity="right"
        android:textCursorDrawable="@drawable/color_cursor" >

        <requestFocus />
    </EditText>

    <Button
        android:id="@+id/cancel"
        android:layout_width="110dp"
        android:layout_height="36dp"
        android:layout_gravity="bottom|left"
        android:layout_marginBottom="23dp"
        android:layout_marginLeft="25dp"
        android:background="@drawable/btn_cancel"
        android:text="" />

    <Button
        android:id="@+id/delivery"
        android:layout_width="110dp"
        android:layout_height="36dp"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="23dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/btn_send"
        android:text="" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="10dp"
        android:layout_height="12dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="78dp"
        android:src="@drawable/text_gae" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="10dp"
        android:layout_height="12dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="157dp"
        android:src="@drawable/text_gae" />

    <TextView
        android:id="@+id/myenergy"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="73dp"
        android:text="80"
        android:gravity="right"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#FF6454"
        android:textSize="15sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="10dp"
        android:layout_height="12dp"
        android:layout_gravity="right|top"
        android:layout_marginRight="56dp"
        android:layout_marginTop="78dp"
        android:src="@drawable/text_won" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="10dp"
        android:layout_height="12dp"
        android:layout_gravity="right|top"
        android:layout_marginRight="56dp"
        android:layout_marginTop="157dp"
        android:src="@drawable/text_won" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:layout_marginRight="70dp"
        android:layout_marginTop="69dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#686868"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|right"
        android:layout_marginRight="70dp"
        android:layout_marginTop="150dp"
        android:textColor="#FF6454"
        android:textSize="24sp" />

</FrameLayout>